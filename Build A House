background(219, 255, 255);

//roof
fill(135, 95, 67);
triangle(215, 25, 360, 155, 62, 155);

//garss
for (var x=0; x<400; x+=100)
{
image(getImage("cute/GrassBlock"),x, 330, 100, 70);
}

//wall
for (var i=60; i<350; i+=30)
{
    for (var y=140; y<350; y+=30)
    {
image(getImage("cute/StoneBlock"),i, y,30,50);
}
}

//door
fill(120, 80, 19);
rect(180, 280, 40, 77);

//windows
for(var w=70; w<350; w+=50)
{
image(getImage("cute/WindowTall"),w, 180, 30,50);
}
